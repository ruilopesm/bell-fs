meta {
  name: Create integrity level
  type: http
  seq: 3
}

post {
  url: {{host}}/levels/integrity
  body: json
  auth: inherit
}

body:json {
  {
    "integrity": {
      "name": "Strong",
      "level": 3
    }
  }
}

script:post-response {
  if (res.getStatus() === 201) {
    let data = res.getBody();
    let integrity = data.integrity;
    
    bru.setEnvVar("integrity_name", integrity.name);
  }
}
